<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <setting name="mapUnderscoreToCamelCase" value="true" />
        <setting name="jdbcTypeForNull" value="VARCHAR"/>
    </settings>

    <typeAliases>
        <typeAlias alias="BatchJob" type="kr.co.pulmuone.batch.esl.domain.model.system.BatchJob" />
        <typeAlias alias="BatchJobExecution" type="kr.co.pulmuone.batch.esl.domain.model.system.BatchJobExecution" />
        <typeAlias alias="SampleMasterVo" type="kr.co.pulmuone.batch.esl.domain.model.sample.SampleMasterVo" />
        <typeAlias alias="EatsslimOrderListVo" type="kr.co.pulmuone.batch.esl.domain.model.order.order.dto.vo.EatsslimOrderListVo" />
    </typeAliases>

    <mappers>
        <mapper resource="mapper/sample/SampleMasterMapper.xml"/>
        <mapper resource="mapper/system/BatchJob.xml"/>
        <mapper resource="mapper/system/BatchJobExecution.xml"/>
        <mapper resource="mapper/send/BatchSendTemplate.xml"/>
        <mapper resource="mapper/order/master/orderErp.xml"/>
    </mappers>
</configuration>