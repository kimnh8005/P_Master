<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.pulmuone.v1.comm.mappers.batch.master.system.SystemBatchJobMapper">

    <select id="getBatchJobStockClosed" parameterType="int" resultType="int">
        SELECT COUNT(1)
          FROM ST_BATCH_JOB_EXECUTION
         WHERE BATCH_NO = #{batchNo}
           AND START_TIME >= CURDATE()
           AND STATUS = 'COMPLETED'
    </select>
</mapper>