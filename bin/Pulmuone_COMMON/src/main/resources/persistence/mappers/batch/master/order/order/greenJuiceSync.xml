<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.pulmuone.v1.comm.mappers.batch.master.order.order.GreenJuiceSyncMapper">

    <!--───────────────────────────────────────────────────────────────────────
     * description      : 주문 상세 번호 조회
     * @
     * @ 수정일          수정자          수정내용
     * @ ──────────────────────────────────────────────────────────────────────
     * @ 2021.08.09     김명진          최초생성
     * @
    ────────────────────────────────────────────────────────────────────────-->
    <select id="getOdOrderDetlByOrderDetlScheduleVo" resultType="long">
        /*  greenJuiceSync.getOdOrderDetlByOrderDetlScheduleVo  */
        SELECT
            OD_ORDER_DETL_ID
        FROM
            OD_ORDER_DETL
        WHERE
            ODID = #{odid}
        AND
            OD_ORDER_DETL_SEQ = #{odOrderDetlSeq}
    </select>
</mapper>